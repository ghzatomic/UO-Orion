function spell_earthquake(){
if (Player.Mana()<30){Orion.Say('.drinkmana');
if (Orion.WaitJournal('Total Mana Refresh!', Orion.Now(), Orion.Now()+100)){
Orion.AddDisplayTimer(300, 19000, 'top', 'circle|bar', 'Potion', 0, 40, '0x0058', 0, 'red');}}
var scroll=Orion.FindType('0x1F65');
if (scroll && scroll.length>0){Orion.UseType('0x1F65');}else{Orion.Cast('57');}
Orion.AddDisplayTimer(100, 5200, 'underChar', 'line|bar', '', 0, 0, '0x0058', 0, 'red');}